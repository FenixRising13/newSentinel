<main class='container'>
    <div class='row'>

        <div class='col l10'>
            {{#each articles}}
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title"></span>
                    <div id='articles'>
                        <p class="articles">{{title}}</p>
                        <br>
                        <a href="https://www.orlandosentinel.com{{link}}">{{title}}</a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>

        <div class='col l2'>
            <div id='notes'>
                Notes<br><br>This is where the notes should go
                {{#each notes}}
                <p class="notes">{{note}}</p>
                {{/each}}
            </div>
        </div>

    </div>
</main>