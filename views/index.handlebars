<main>
  <div class='row'>
    <div class='col s10'>
      {{#each articles}}
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"></span>
          <div id='articles'>
            <p class="articles">{{title}}</p>
            <br>
            <a href="https://www.orlandosentinel.com{{link}}">{{title}}</a>
          </div>
        </div>
      </div>
      {{/each}}
    </div>

    <div class='card col s2'>
      <div id='notes'>
        <div class="card-title">Notes</div>
        <hr />
        <div class="card-content">This is where the notes should go.</div>
        {{#each notes}}
        <p class="notes">{{note}}</p>
        {{/each}}
      </div>
    </div>

  </div>
</main>